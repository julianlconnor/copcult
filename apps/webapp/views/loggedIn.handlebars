<h1>Logged In</h1>
<span>{{ user.instagramUsername }}</span>
<a href="/logout">Logout</a>
<div id="content"></div>


<script>
  if ( !window.jaded ) {
    window.jaded = {};
    window.jaded.user = {};
  }

  window.jaded.user = JSON.parse('{{{ user }}}');
</script>

{{# if SERVE_COMPILED }}
  <script src="/js/jaded.min.js"></script>
{{ else }}
  <script src="/shared/lib/requirejs/require.js"></script>
  <script src="/webapp/public/js/main.js"></script>
  <script>require(['webapp/public/js/app'], function(app) {});</script>
{{/ if }}
